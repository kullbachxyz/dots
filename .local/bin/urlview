#!/bin/bash
urls=$(grep -Eo 'https?://[^[:space:]>"]+' | sort -u)

if [ -z "$urls" ]; then
    notify-send "No URL found"
else
    selected=$(echo "$urls" | dmenu -l 10 -p "Open URL:")
    if [ -n "$selected" ]; then
        xdg-open "$selected"
        notify-send "Opening URL" "$selected"
    fi
fi
